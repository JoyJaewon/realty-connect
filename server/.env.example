# MongoDB Atlas 연결 정보
MONGODB_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/realty-connect?retryWrites=true&w=majority

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your-super-secret-refresh-key-here
JWT_REFRESH_EXPIRES_IN=7d

# 서버 설정
PORT=5000
NODE_ENV=development

# CORS 설정
CORS_ORIGIN=http://localhost:3000 