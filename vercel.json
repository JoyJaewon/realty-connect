{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": "client/.next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://realty-connect-backend.railway.app",
    "NEXT_PUBLIC_SOCKET_URL": "https://realty-connect-backend.railway.app"
  }
} 